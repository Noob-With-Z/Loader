print("Thanks For Use")

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/Noob-With-Z/Orion/main/source')))()
OrionLib:MakeNotification({
    Name = "Warning",
    Content = "Script Loading...",
    Image = "rbxassetid://15596611355",
    Time = 5
})

local listPre = {
    -- Murder Mystery 2
    [66654135] = "loadstring(game:HttpGet('https://raw.githubusercontent.com/Noob-With-Z/Scripts/main/mm2'))()",
}

if listPre[game.GameId] ~= nil then
    loadstring(listPre[tonumber(game.GameId)])()
else
    -- Carregar script universal se nenhum script espec√≠fico for encontrado
    loadstring(game:HttpGet('https://raw.githubusercontent.com/Noob-With-Z/Scripts/main/univ'))()
end
